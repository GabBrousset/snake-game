{
  "expo": {
    "name": "Snek",
    "slug": "snek",
    "version": "1.1.2",
    "description": "A minimalistic take on the classical snake game.",
    "privacy": "public",
    "orientation": "portrait",
    "sdkVersion": "39.0.0",
    "icon": "./assets/icon.png",
    "platforms": ["ios", "android"],
    "githubUrl": "https://github.com/GabBrousset/snake-game-rn",
    "backgroundColor": "#000000",
    "primaryColor": "#1e1e1e",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e1e1e"
    },
    "updates": {
      "fallbackToCacheTimeout": 15
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.trescool.snek",
      "buildNumber": "1.1.2"
    },
    "android": {
      "package": "io.trescool.snek",
      "versionCode": 2,
      "permissions": [ "VIBRATE" ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
